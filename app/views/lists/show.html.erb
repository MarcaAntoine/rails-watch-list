
<h2><%= @list.name %></h2>
<ul class="list-group">
  <% @list.movies.each do |movie| %>
    <li class="list-group-item">
      <p><%= movie.title %></p>
      <p><%= movie.overview %></p>
      <p><%= movie.poster_url %></p>
      <p><%= movie.rating %></p>
      <%= cl_image_tag @list.photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>
    </li>
</ul>
